@model Student[];


<br/>

    @foreach(Student stu in Model){
        
        <div class="alert alert-primary">
            <span>
                 ID: @stu.id
            </span>
            <span>
                 Name: @stu.name
            </span>
            <span>
                 Grade: @stu.grade.ToString("F2")
            </span>
        </div>
       
    }

















    @section header{
    <a href="@Url.Action("Index","Home")" class="btn btn-primary">Home</a>
    <a href="@Url.Action("Add","Home")" class="btn btn-info">Add</a>
    <a href="@Url.Action("Edit","Home")" class="btn btn-info">Edit</a>
    <a href="@Url.Action("Delete","Home")" class="btn btn-danger">Delete</a>
    <a href="@Url.Action("Show","Home")" class="btn btn-warning">Show</a>

    }


